{
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorTheme": "Dracula Theme",
    "editor.rulers": [80, 160],
    "editor.fontFamily": "Operator Mono Book, Menlo, Monaco, 'Courier New', monospace",
    "editor.inlayHints.fontFamily": "Operator Mono Book",
    "terminal.integrated.fontFamily": "Operator Mono Book",
    "editor.fontSize": 16,
    "window.zoomLevel": 1.1,
    "workbench.editor.labelFormat": "short",
    "window.titleBarStyle": "custom",
    "apc.electron": {
        "titleBarStyle": "hiddenInset",
        "trafficLightPosition": {
            "x": 10,
            "y": 10
        }
    },
    "apc.activityBar": {
        "size": 90,
        "itemSize": 33
    },
    "apc.stylesheet": {
        ".mac .sidebar .composite-bar-container": "padding-left: 70px",
        ".mac .inline-titlebar-placeholder": "padding-left: 0 !important"
    },
    "workbench.activityBar.location": "hidden",
    "files.watcherExclude": {
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/*.code-search": true,
        "**/*.db": true,
        "**/*.log": true,
        "**/*.orig": true,
        "**/*.pyc": true,
        "**/*.so": true,
        "**/*.swo": true,
        "**/*.swp": true,
        "**/*~": true,
        "**/blob": true,
        "**/bower_components": true,
        "**/converter": true,
        "**/docker/*.sql": true,
        "**/docker/es_dump/": true,
        "**/hooks": true,
        "**/log": true,
        "**/media": true,
        "**/node_modules": true,
        "**/populate": true,
        "**/zelp": true,
        "**/zmail": true
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/*.code-search": true,
        "**/*.db": true,
        "**/*.orig": true,
        "**/*.pyc": true,
        "**/*.so": true,
        "**/*.swo": true,
        "**/*.swp": true,
        "**/*~": true,
        "**/blob": true,
        "**/bower_components": true,
        "**/converter": true,
        "**/docker/*.sql": true,
        "**/docker/es_dump/": true,
        "**/hooks": true,
        "**/media": true,
        "**/node_modules": true,
        "**/populate": true,
        "**/zelp": true,
        "**/zmail": true
    },
    "search.exclude": {
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/*.code-search": true,
        "**/*.db": true,
        "**/*.orig": true,
        "**/*.pyc": true,
        "**/*.so": true,
        "**/*.swo": true,
        "**/*.swp": true,
        "**/*~": true,
        "**/blob": true,
        "**/bower_components": true,
        "**/converter": true,
        "**/docker/*.sql": true,
        "**/docker/es_dump/": true,
        "**/hooks": true,
        "**/media": true,
        "**/node_modules": true,
        "**/populate": true,
        "**/zelp": true,
        "**/zmail": true
    },
    "windsurfPyright.analysis.diagnosticMode": "workspace",
    "windsurfPyright.analysis.exclude": [
        "**/__pycache__",
        "**/.DS_Store",
        "**/.git",
        "**/.hg",
